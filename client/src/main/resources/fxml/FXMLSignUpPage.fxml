         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Button fx:id="SignUP_BtnClose" layoutX="342.0" layoutY="-1.0" mnemonicParsing="false" onAction="#close" prefHeight="36.0" prefWidth="32.0" text="X">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="SignUp_BtnMin" layoutX="297.0" layoutY="-1.0" mnemonicParsing="false" onAction="#minimize" prefHeight="36.0" prefWidth="32.0" text="_" />
