<AnchorPane id="AnchorPane" fx:id="anchorSignup" prefHeight="532.0" prefWidth="392.0" style="-fx-background-color: #9c2b27;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.FXMLSignUpPageController">
